    "dayjs": "^1.11.6",
    "element-plus": "^2.2.20",
    "fast-average-color": "^9.1.1",
    "jwt-decode": "^3.1.2",
    "pinia": "^2.0.23",
    "uuid": "^9.0.0",
    "validator": "^13.7.0",
    "vue": "^3.2.41",
    "vue-debounce": "^4.0.0",
    "vue-loading-overlay": "^6.0.2",
    "vue-router": "^4.1.5"
